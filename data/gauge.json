{
  "id": "gauge",
  "name": "Gauge",
  "executable_name": "gauge",
  "category": "Development",
  "description": "To be a better testing tool, from version 0.9.0, Gauge collects anonymous usage statistics (telemetry). The data is public and thereâ€™s an easy way to opt out.",
  "links": {
    "main": "https://gauge.org/",
    "telemetry": "https://manpage.gauge.org/gauge_telemetry.html",
    "privacy": "https://www.thoughtworks.com/privacy-policy"
  },
  "telemetry": [
    {
      "id": "usage-data",
      "name": "Usage data",
      "is_official": true,
      "target": {
        "env": {
          "path": {
            "default": "GAUGE_TELEMETRY_ENABLED"
          },
          "value": {
            "opt_out": "false",
            "opt_in": null
          }
        },
        "exec": {
          "path": {
            "default": "gauge"
          },
          "value": {
            "opt_out": "telemetry off",
            "opt_in": "telemetry on"
          }
        }
      }
    }
  ]
}