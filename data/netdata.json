{
  "id": "netdata",
  "name": "Netdata",
  "executable_name": [
    "netdata"
  ],
  "category_id": "cloud",
  "category_name": "Cloud",
  "description": "Starting with v1.12, Netdata collects anonymous usage information by default and sends it to Google Analytics.",
  "links": {
    "main": "https://www.netdata.cloud",
    "telemetry": "https://learn.netdata.cloud/docs/agent/anonymous-statistics",
    "privacy": "https://learn.netdata.cloud/docs/agent/privacy-policy/"
  },
  "telemetry": [
    {
      "id": "usage-data",
      "name": "Usage data",
      "is_official": true,
      "target": {
        "env": {
          "scope": {
            "process": {
              "path": {
                "default": "DO_NOT_TRACK"
              },
              "value": {
                "opt_out": "1",
                "opt_in": null
              }
            }
          }
        }
      }
    }
  ]
}